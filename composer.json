{
    "name": "sourcefli/laravel-carbon-helpers",
    "description": "Carbon helpers and collection ",
    "keywords": [
        "sourcefli",
        "laravel",
        "laravel-carbon-helpers",
        "carbon"
    ],
    "homepage": "https://github.com/sourcefli/laravel-carbon-helpers",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan Havens",
            "email": "mail@jhavens.tech",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "nesbot/carbon": "^2.57.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0",
        "illuminate/collections": "^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Sourcefli\\CarbonHelpers\\": "src",
        },
        "files": [
           "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sourcefli\\CarbonHelpers\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sourcefli\\CarbonHelpers\\CarbonHelpersServiceProvider"
            ],
            "aliases": {
                "CarbonHelpers": "Sourcefli\\CarbonHelpers\\Facades\\CarbonHelpers"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
